{
  "name": "obi",
  "displayName": "Object Builder for IBM i (OBI)",
  "description": "Automated build tool for IBM i",
  "publisher": "andreas-prouza",
  "version": "0.6.13",
  "icon": "asserts/img/ext/logo.png",
  "keywords": [
    "ibm i",
    "ibmi",
    "iseries",
    "as400",
    "rpgle",
    "ile",
    "build"
  ],
  "author": {
    "name": "Andreas Prouza",
    "url": "https://www.prouza.at"
  },
  "license": "MIT",
  "repository": {
    "url": "https://github.com/andreas-prouza/obi-extension"
  },
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "obi.show_changes",
        "title": "%obi.show_changes%",
        "icon": "$(check-all)",
        "when": "obi.contains_obi_project && obi.valid-config"
      },
      {
        "command": "obi.show_single_changes",
        "title": "%obi.show_single_changes%",
        "icon": "$(check)",
        "when": "obi.contains_obi_project && obi.valid-config"
      },
      {
        "command": "obi.run_build",
        "title": "%obi.run_build%",
        "icon": "$(run-all)",
        "when": "obi.contains_obi_project && obi.valid-config"
      },
      {
        "command": "obi.run_single_build",
        "title": "%obi.run_single_build%",
        "icon": "$(run)",
        "when": "obi.contains_obi_project && obi.valid-config"
      },
      {
        "command": "obi.source-filter.update",
        "title": "%obi.source-filter.update%",
        "icon": "$(refresh)",
        "when": "obi.contains_obi_project && obi.valid-config"
      },
      {
        "command": "obi.source-filter.add",
        "title": "%obi.source-filter.add%",
        "icon": "$(add)",
        "when": "obi.contains_obi_project && obi.valid-config"
      },
      {
        "command": "obi.get-remote-compiled-object-list",
        "title": "%obi.get-remote-compiled-object-list%",
        "icon": "$(arrow-circle-down)",
        "when": "obi.contains_obi_project && obi.valid-config && false"
      },
      {
        "command": "obi.copy-profile-config",
        "title": "%obi.copy-profile-config%",
        "icon": "$(person-add)",
        "when": "obi.contains_obi_project && obi.valid-config"
      },
      {
        "command": "obi.reset-compiled-object-list",
        "title": "%obi.reset-compiled-object-list%",
        "icon": "$(extensions)",
        "when": "obi.contains_obi_project && obi.valid-config"
      },
      {
        "command": "obi.get-remote-source-list",
        "title": "%obi.get-remote-source-list%",
        "icon": "$(arrow-down)",
        "when": "obi.contains_obi_project && obi.valid-config && false"
      },
      {
        "command": "obi.check-remote-sources",
        "title": "%obi.check-remote-sources%",
        "icon": "$(tasklist)",
        "when": "obi.contains_obi_project && obi.valid-config && false"
      },
      {
        "command": "obi.controller.config",
        "title": "%obi.controller.config%",
        "icon": "$(settings-gear)",
        "when": "obi.contains_obi_project"
      },
      {
        "command": "obi.controller.dependency-list",
        "title": "%obi.controller.dependency-list%",
        "icon": "$(outline-view-icon)",
        "when": "obi.contains_obi_project && obi.valid-config"
      },
      {
        "command": "obi.source.edit-compile-config",
        "title": "%obi.source.edit-compile-config%",
        "icon": "$(settings-gear)",
        "when": "obi.contains_obi_project"
      },
      {
        "command": "obi.transfer-all",
        "title": "%obi.transfer-all%",
        "icon": "$(cloud-upload)",
        "when": "obi.contains_obi_project && obi.valid-config"
      },
      {
        "command": "obi.source-filter.show-view",
        "title": "%obi.source-filter.show-view%",
        "icon": "$(preview)",
        "when": "obi.contains_obi_project && obi.valid-config"
      },
      {
        "command": "obi.source-filter.edit-config",
        "title": "%obi.source-filter.edit-config%",
        "icon": "$(settings-gear)",
        "when": "obi.contains_obi_project && obi.valid-config"
      },
      {
        "command": "obi.source-filter.delete-config",
        "title": "%obi.source-filter.delete-config%",
        "icon": "$(trash)",
        "when": "obi.contains_obi_project && obi.valid-config"
      },
      {
        "command": "obi.source-filter.view-source-infos",
        "title": "%obi.source-filter.view-source-infos%",
        "icon": "$(preview)",
        "when": "obi.contains_obi_project && obi.valid-config"
      },
      {
        "command": "obi.source-filter.maintain-source-infos",
        "title": "%obi.source-filter.maintain-source-infos%",
        "icon": "$(settings-gear)",
        "when": "obi.contains_obi_project && obi.valid-config"
      },
      {
        "command": "obi.source-filter.add-source-file",
        "title": "%obi.source-filter.add-source-file%",
        "icon": "$(add)",
        "when": "obi.contains_obi_project && obi.valid-config"
      },
      {
        "command": "obi.source-filter.add-source-member",
        "title": "%obi.source-filter.add-source-member%",
        "icon": "$(add)",
        "when": "obi.contains_obi_project && obi.valid-config"
      },
      {
        "command": "obi.source-filter.change-source-description",
        "title": "%obi.source-filter.change-source-description%",
        "icon": "$(preview)",
        "when": "obi.contains_obi_project && obi.valid-config"
      },
      {
        "command": "obi.source-filter.rename-source-member",
        "title": "%obi.source-filter.rename-source-member%",
        "when": "obi.contains_obi_project && obi.valid-config"
      },
      {
        "command": "obi.source-filter.delete-source-member",
        "title": "%obi.source-filter.delete-source-member%",
        "when": "obi.contains_obi_project && obi.valid-config"
      },
      {
        "command": "obi.source.maintain-source-dependency",
        "title": "%obi.source.maintain-source-dependency%",
        "icon": "$(combine)",
        "when": "obi.contains_obi_project && obi.valid-config"
      },
      {
        "command": "obi.deployment.maintain",
        "title": "%obi.deployment.maintain%",
        "icon": "$(settings-gear)",
        "when": "1==2 && obi.contains_obi_project && obi.valid-config"
      },
      {
        "command": "obi.build-history.update",
        "title": "Update Build History"
      },
      {
        "command": "obi.build-history.delete-item",
        "title": "Delete Build History Item",
        "icon": "$(trash)"
      },
      {
        "command": "obi.build-history.delete-date",
        "title": "Delete Build History for Date",
        "icon": "$(trash)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "obi.source-filter.add",
          "when": "obi.contains_obi_project && obi.valid-config && view == obi.source-filter",
          "group": "navigation@1"
        },
        {
          "command": "obi.source-filter.view-source-infos",
          "when": "obi.contains_obi_project && obi.valid-config && view == obi.source-filter",
          "group": "navigation@2"
        },
        {
          "command": "obi.source-filter.maintain-source-infos",
          "when": "obi.contains_obi_project && obi.valid-config && view == obi.source-filter",
          "group": "navigation@3"
        },
        {
          "command": "obi.source-filter.update",
          "when": "obi.contains_obi_project && obi.valid-config && view == obi.source-filter",
          "group": "navigation@5"
        },
        {
          "command": "obi.get-remote-source-list",
          "when": "false && obi.contains_obi_project && (view == obi.controller || (!obi.valid-config && view == obi-config-invalid))",
          "group": "navigation@20"
        },
        {
          "command": "obi.get-remote-compiled-object-list",
          "when": "false && obi.contains_obi_project && (view == obi.controller || (!obi.valid-config && view == obi-config-invalid))",
          "group": "navigation@30"
        },
        {
          "command": "obi.reset-compiled-object-list",
          "when": "obi.contains_obi_project && (view == obi.controller || (!obi.valid-config && view == obi-config-invalid))",
          "group": "navigation@40"
        },
        {
          "command": "obi.check-remote-sources",
          "when": "false && obi.contains_obi_project && (view == obi.controller || (!obi.valid-config && view == obi-config-invalid))",
          "group": "navigation@50"
        },
        {
          "command": "obi.transfer-all",
          "when": "obi.contains_obi_project && (view == obi.controller || (!obi.valid-config && view == obi-config-invalid))",
          "group": "navigation@60"
        },
        {
          "command": "obi.controller.dependency-list",
          "when": "obi.contains_obi_project && (view == obi.controller || (!obi.valid-config && view == obi-config-invalid))",
          "group": "navigation@65"
        },
        {
          "command": "obi.controller.config",
          "when": "obi.contains_obi_project && (view == obi.controller || (!obi.valid-config && view == obi-config-invalid))",
          "group": "navigation@70"
        },
        {
          "command": "obi.deployment.maintain",
          "when": "obi.contains_obi_project && obi.valid-config && view == obi.deployment",
          "group": "navigation@1"
        }
      ],
      "editor/title/context": [
        {
          "command": "obi.source.edit-compile-config",
          "when": "obi.contains_obi_project"
        },
        {
          "command": "obi.source-filter.change-source-description",
          "when": "obi.contains_obi_project"
        },
        {
          "command": "obi.source.maintain-source-dependency",
          "when": "obi.contains_obi_project"
        }
      ],
      "explorer/context": [
        {
          "command": "obi.source.edit-compile-config",
          "when": "obi.contains_obi_project"
        },
        {
          "command": "obi.source-filter.change-source-description",
          "when": "obi.contains_obi_project"
        },
        {
          "command": "obi.source.maintain-source-dependency",
          "when": "obi.contains_obi_project"
        }
      ],
      "view/item/context": [
        {
          "command": "obi.source-filter.show-view",
          "when": "obi.contains_obi_project && view == obi.source-filter && viewItem == source-list",
          "group": "inline@0"
        },
        {
          "command": "obi.source-filter.edit-config",
          "when": "obi.contains_obi_project && view == obi.source-filter && viewItem == source-list",
          "group": "inline@2"
        },
        {
          "command": "obi.source-filter.delete-config",
          "when": "obi.contains_obi_project && view == obi.source-filter && viewItem == source-list",
          "group": "inline@3"
        },
        {
          "command": "obi.source.edit-compile-config",
          "when": "obi.contains_obi_project && view == obi.source-filter && viewItem == obi-source",
          "group": "inline@0"
        },
        {
          "command": "obi.source.maintain-source-dependency",
          "when": "obi.contains_obi_project && view == obi.source-filter && viewItem == obi-source",
          "group": "inline@1"
        },
        {
          "command": "obi.source-filter.change-source-description",
          "when": "obi.contains_obi_project && view == obi.source-filter && viewItem == obi-source",
          "group": "inline@1"
        },
        {
          "command": "obi.source-filter.rename-source-member",
          "when": "obi.contains_obi_project && view == obi.source-filter && viewItem == obi-source"
        },
        {
          "command": "obi.source-filter.delete-source-member",
          "when": "obi.contains_obi_project && view == obi.source-filter && viewItem == obi-source"
        },
        {
          "command": "obi.source-filter.add-source-member",
          "when": "obi.contains_obi_project && view == obi.source-filter && viewItem == source-file",
          "group": "inline@0"
        },
        {
          "command": "obi.source-filter.add-source-file",
          "when": "obi.contains_obi_project && view == obi.source-filter && viewItem == source-lib",
          "group": "inline@0"
        },
        {
          "command": "obi.build-history.delete-item",
          "when": "view == obi.build-history && viewItem == buildHistoryFile",
          "group": "inline"
        },
        {
          "command": "obi.build-history.delete-date",
          "when": "view == obi.build-history && viewItem == buildHistoryDate",
          "group": "inline"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "obi-bar",
          "title": "Object Builder for i",
          "icon": "$(code)"
        }
      ]
    },
    "views": {
      "obi-bar": [
        {
          "type": "webview",
          "id": "obi.controller",
          "name": "OBI Build",
          "when": "obi.contains_obi_project && obi.valid-config"
        },
        {
          "type": "tree",
          "id": "obi.build-history",
          "name": "Build history",
          "when": "obi.contains_obi_project && obi.valid-config"
        },
        {
          "type": "tree",
          "id": "obi.source-filter",
          "name": "%Source filters%",
          "when": "obi.contains_obi_project && obi.valid-config"
        },
        {
          "type": "webview",
          "id": "obi.quick-settings",
          "name": "%Quick settings%",
          "when": "false && obi.contains_obi_project && obi.valid-config"
        },
        {
          "type": "webview",
          "id": "obi.deployment",
          "name": "%deployment%",
          "when": "1==2 && obi.contains_obi_project && obi.valid-config"
        },
        {
          "type": "webview",
          "id": "obi-welcome",
          "name": "%Welcome%",
          "when": "obi.contains_obi_project == false"
        },
        {
          "type": "webview",
          "id": "obi-config-invalid",
          "name": "%Invalid config%",
          "when": "obi.contains_obi_project && !obi.valid-config"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package --omit=dev",
    "compile": "node ./esbuild.js",
    "package": "NODE_ENV=production node ./esbuild.js",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test",
    "postinstall": "rm -rf node_modules/ssh2/test"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@types/vscode": "^1.66.0",
    "@types/vscode-webview": "^1.57.5",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "latest",
    "ovsx": "latest"
  },
  "dependencies": {
    "@emnapi/core": "^1.4.3",
    "@emnapi/runtime": "^1.4.3",
    "@tybys/wasm-util": "^0.9.0",
    "@vscode/webview-ui-toolkit": "^1.4.0",
    "deepmerge": "^4.3.1",
    "fs-extra": "^11.2.0",
    "hasha": "^6.0.0",
    "node-ssh": "^13.2.0",
    "nunjucks": "^3.2.4",
    "smol-toml": "^1.3.0",
    "wildcard-match": "latest",
    "winston": "latest",
    "winston-transport": "latest",
    "esbuild": "^0.25.10"
  }
}
